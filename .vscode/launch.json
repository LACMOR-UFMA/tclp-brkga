{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "g++ debug",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/plct-brkga",
        "args": ["instances/AC.txt", "10", "10", "10", "1", "4"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "preLaunchTask": "C/C++: g++ build active file",
        "miDebuggerPath": "/usr/bin/gdb"
      }
    ]
  }